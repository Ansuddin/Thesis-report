@article{Hedin2014,
abstract = {JavaScript drives the evolution of the web into a powerful application$\backslash$nplatform. Increasingly, web applications combine services from different$\backslash$nproviders. The script inclusion mechanism routinely turns barebone web$\backslash$npages into fully-fledged services built up from third-party code. Such$\backslash$ncode provides a range of facilities from helper utilities (such as jQuery)$\backslash$nto readily available services (such as Google Analytics and Tynt). This$\backslash$nposes a security challenge of ensuring that the integrated third-party code$\backslash$nrespects security and privacy.$\backslash$nThis paper presents$\backslash$nJSFlow$\backslash$n, a security-enhanced JavaScript interpreter$\backslash$nfor fine-grained tracking of information flow. We show how to resolve$\backslash$npractical challenges for enforcing information-flow policies for the full$\backslash$nJavaScript language, as well as the challenges of tracking information in$\backslash$nthe presence of libraries, as provided by browser APIs.$\backslash$nThe interpreter is itself written in JavaScript, which enables deploy-$\backslash$nment as a Firefox extension. Using this extension we have performed$\backslash$npractical studies to provide in-depth understanding of information manip-$\backslash$nulation by third-party scripts such as Google Analytics. The experiments$\backslash$nshow that different sites intended to provide similar services effectuate$\backslash$nrather different security policies for the user's sensitive information: some$\backslash$nensure it does not leave the browser, others share it with the originating$\backslash$nserver, while yet others freely propagate it to third parties.},
author = {Hedin, Daniel and Birgisson, Arnar and Bello, Luciano and Sabelfeld, Andrei},
doi = {10.1145/2554850.2554909},
file = {:C$\backslash$:/Users/ansuddin/Dropbox/Thesis/Andet/Materiale/Papers/Relevant/+JSFlow - Tracking Information Flow in JavaScript and its APIs.pdf:pdf},
isbn = {9781450324694},
issn = {1069-417X},
journal = {Proceedings of the 29th Annual ACM Symposium on Applied Computing - SAC '14},
keywords = {dynamic analysis,information flow,javascript},
pages = {1663--1671},
title = {{[103]JSFlow{\_} Tracking information flow in JavaScript and its APIs}},
url = {http://dl.acm.org/citation.cfm?doid=2554850.2554909},
year = {2014}
}



